<%= form_for @image do |f| %>

	<%= f.label :title, "Title of image" %>
	<%= f.text_field :title %><br><br>

	<%= f.label :url, "File name" %>
	<%= f.text_field :url %><br><br>

	<%= f.hidden_field :board_id, value: session[:board_id] %>
	<%= f.hidden_field :user_id, value: session[:user_id] %>

	<%= f.submit %>

<% end %>